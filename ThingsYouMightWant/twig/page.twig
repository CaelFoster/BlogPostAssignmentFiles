{% extends 'base.twig' %}
{% block body_classes %}
	{% set page_color = post.meta('page_color') %}
	{{ page_color }}
{% endblock %}
{% block content %}
	{% block page_content %}
		{# {% include 'components/contact-form.twig' %} #}
		{% for module in post.meta('sections') %}
			{% include [
				'modules/' ~ module.acf_fc_layout ~ '.twig',
				'modules/missing-module.twig'
			] with {
				module_name: module.acf_fc_layout
			} %}
		{% endfor %}
	{% endblock %}
{% endblock %}
